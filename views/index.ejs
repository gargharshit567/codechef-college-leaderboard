<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <meta charset="utf-8">
  <title></title>
  <link rel="stylesheet" href="/css/styles.css">
</head>

<body>
  <div>


    <div class="row">
      <div class="sno">S.NO.</div>
      <div class="name">Name</div>
      <div class="score">Score</div>
      <div class="rank">Rank</div>
      <div class="division">division</div>
      <!-- <h1>hello</h1> -->
    </div>
    <% var item; %>
    <% for(var i=(paras.page-1)*paras.limit;i<=data.length-1 && i<=paras.page*paras.limit ;i++) {%>
    <% item= data[i]; %>
    <div id="container">
      <div class="row">
        <div class="sno"><%= i+1 %></div>
        <div class="name"><%=item.username%></div>
        <div class="score"><%=item.L_Score%></div>
        <div class="rank"><%=item.L_rank%></div>
        <div class="division"><%= item.division %></div>
        <!-- <h1>hello</h1> -->
      </div>
    </div>
    <% } %>


  </div>
  <% for(var i=1;i<=paras.pages;i++){%>

  <a  href="\?page=<%=i%>&limit=<%=paras.limit%>"> <div class="pages"> <%=i%> - <%=paras.pages%> </div> </a>
  <% }%>
</body>

</html>
